@article{cunninghamStructuralTransectMongolian1996,
  title = {A Structural Transect across the {{Mongolian Western Altai}}: {{Active}} Transpressional Mountain Building in Central {{Asia}}},
  shorttitle = {A Structural Transect across the {{Mongolian Western Altai}}},
