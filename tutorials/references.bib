@article{johndrowBayesShrinkageGWAS2018,
  title = {Bayes {{Shrinkage}} at {{GWAS}} Scale: {{Convergence}} and {{Approximation Theory}} of a {{Scalable MCMC Algorithm}} for the {{Horseshoe Prior}}},
