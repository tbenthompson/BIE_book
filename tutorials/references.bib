  author = {Frankel, Kurt L. and Wegmann, Karl W. and Bayasgalan, Amgalan and Carson, Robert J. and Bader, Nicholas E. and Adiya, Tsolmon and Bolor, Erdenebat and Durfey, Chelsea C. and Otgonkhuu, Jargal and Sprajcar, Jodi and Sweeney, Kristin E. and Walker, Richard T. and Marstellar, Tina L. and Gregory, Laura},
