@article{cunninghamStructuralFrameworkMajor2003,
  title = {Structural Framework of a Major Intracontinental Orogenic Termination Zone: The Easternmost {{Tien Shan}}, {{China}}},
  shorttitle = {Structural Framework of a Major Intracontinental Orogenic Termination Zone},
  author = {Cunningham, Dickson and Owen, LewisA. and Snee, LarryW. and Jiliang, Li},
  year = {2003},
