@article{dumbserADERWENOFiniteVolume2013,
  title = {{{ADER-WENO Finite Volume Schemes}} with {{Space-Time Adaptive Mesh Refinement}}},
  author = {Dumbser, Michael and Zanotti, Olindo and Hidalgo, Arturo and Balsara, Dinshaw S.},
  year = {2013},
