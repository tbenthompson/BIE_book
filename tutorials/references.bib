  author = {Leloup, Philippe Herv{\'e} and Lacassin, Robin and Tapponnier, Paul and Sch{\"a}rer, Urs and Zhong, Dalai and Liu, Xiaohan and Zhang, Liangshang and Ji, Shaocheng and Trinh, Phan Trong},
