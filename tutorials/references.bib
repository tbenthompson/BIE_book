}

@article{leloupShearHeatingContinental1999,
  title = {Shear Heating in Continental Strike-Slip Shear Zones:Model and Field Examples},
  shorttitle = {Shear Heating in Continental Strike-Slip Shear Zones},
  author = {Leloup, P. H. and Ricard, Y. and Battaglia, J. and Lacassin, R.},
  year = {1999},
