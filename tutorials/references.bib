@article{metivierGlobalCoseismicDeformations2014,
  title = {Global Coseismic Deformations, {{GNSS}} Time Series Analysis, and Earthquake Scaling Laws: {{Co-seismicity}}, {{GNSS}} and Scaling Laws},
  shorttitle = {Global Coseismic Deformations, {{GNSS}} Time Series Analysis, and Earthquake Scaling Laws},
  author = {M{\'e}tivier, Laurent and Collilieux, Xavier and Lercier, Daphn{\'e} and Altamimi, Zuheir and Beauducel, Fran{\c c}ois},
