@article{robinsonEvidenceQuaternarySlip2009,
  title = {Evidence against {{Quaternary}} Slip on the Northern {{Karakorum Fault}} Suggests Kinematic Reorganization at the Western End of the {{Himalayan}}\textendash{{Tibetan}} Orogen},
